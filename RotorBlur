while #game.Workspace.Other:GetChildren() < 10 do
	while wait() do
		if script.Parent.Parent.Parent.Parent.Engine.SpeedYes.Value > 20 then
			script.Parent.Parent.Down1.Transparency = 1
			script.Parent.Parent.Down2.Transparency = 1
			script.Parent.Parent.Down3.Transparency = 1
			script.Parent.Parent.Down4.Transparency = 1
			script.Parent.Parent.Down5.Transparency = 1
			script.Parent.Parent.Down6.Transparency = 1
			script.Parent.Parent.Up1.Transparency = 0
			script.Parent.Parent.Up2.Transparency = 0
			script.Parent.Parent.Up3.Transparency = 0
			script.Parent.Parent.Up4.Transparency = 0
			script.Parent.Parent.Up5.Transparency = 0
			script.Parent.Parent.Up6.Transparency = 0
			script.Parent.Parent.Spin1.One.Transparency = 0
			script.Parent.Parent.Spin1.Two.Transparency = 0
			script.Parent.Parent.Spin1.Three.Transparency = 0
			script.Parent.Parent.Spin1.Four.Transparency = 0
			script.Parent.Parent.Spin2.One.Transparency = 0.5
			script.Parent.Parent.Spin2.Two.Transparency = 0.5
			script.Parent.Parent.Spin2.Three.Transparency = 0.5
			script.Parent.Parent.Spin2.Four.Transparency = 0.5
		else
			script.Parent.Parent.Down1.Transparency = 0
			script.Parent.Parent.Down2.Transparency = 0
			script.Parent.Parent.Down3.Transparency = 0
			script.Parent.Parent.Down4.Transparency = 0
			script.Parent.Parent.Down5.Transparency = 0
			script.Parent.Parent.Down6.Transparency = 0
			script.Parent.Parent.Up1.Transparency = 1
			script.Parent.Parent.Up2.Transparency = 1
			script.Parent.Parent.Up3.Transparency = 1
			script.Parent.Parent.Up4.Transparency = 1
			script.Parent.Parent.Up5.Transparency = 1
			script.Parent.Parent.Up6.Transparency = 1
			script.Parent.Parent.Spin1.One.Transparency = 1
			script.Parent.Parent.Spin1.Two.Transparency = 1
			script.Parent.Parent.Spin1.Three.Transparency = 1
			script.Parent.Parent.Spin1.Four.Transparency = 1
			script.Parent.Parent.Spin2.One.Transparency = 1
			script.Parent.Parent.Spin2.Two.Transparency = 1
			script.Parent.Parent.Spin2.Three.Transparency = 1
			script.Parent.Parent.Spin2.Four.Transparency = 1
			wait()
		end
	end
end
