local speed = 0
while true do
	if script.Parent ~= nil then 
		speed = math.floor(math.sqrt((script.Parent.Velocity.x^2)+(script.Parent.Velocity.y^2)+(script.Parent.Velocity.z^2)))
		script.Parent.SpeedYes.Value = speed
	elseif script.Parent == nil then 
		script.Parent.SpeedYes.Value = speed

	end 
	wait() 
end 
